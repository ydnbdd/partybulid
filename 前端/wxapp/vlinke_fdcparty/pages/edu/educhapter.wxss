.chapterlisttil {
    padding: 20rpx 30rpx;
    font-size: 40rpx;
    border-bottom: 1rpx solid #ddd;
}

.weui-badge {
    float: right;
    margin-top: 5rpx;
}

.educhapterfoot {
    position: fixed;
    bottom: 0rpx;
    height: 120rpx;
    width: 750rpx;
    text-align: center;
    display: flex;
    justify-content: space-between;
    background-color: #e5e5e5;
    line-height: 120rpx;
}

.educhapterfoot navigator {
    width: 250rpx;
    border-left: 1rpx solid #eee;
    border-right: 1rpx solid #eee;
}