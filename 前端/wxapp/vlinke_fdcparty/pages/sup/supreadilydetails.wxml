<view class="page">
    <view class="wxapp-detail bgfff">
        <view class="wxapp-detail-til">{{supreadily.title}}</view>
        <view class="wxapp-detail-desc">
        {{supreadily.createtime}}
    </view>
        <view class="wxapp-detail-con" style="margin-bottom: 30rpx;">
            <rich-text bindtap="tap" nodes="{{supreadily.details}}"></rich-text>
        </view>
        <view class="wxapp-detail-con" style="margin-bottom: 30rpx;">
            <image class="suppicitem" mode="widthFix" src="{{item}}" wx:for="{{supreadily.picall}}" wx:key="id"></image>
        </view>
    </view>
</view>
<import src="/we7/pages/templates/footer.wxml"></import>
<template is="footerWx" data="{{...tabBar}}"></template>
