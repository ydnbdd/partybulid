    height: 36rpx;
    display: block;
    margin: 0rpx;
    padding: 0rpx;
}

.bbs-box-reply {
    background-color: #f2f2f2;
    border: 20rpx solid #f2f2f2;
    margin-top: 2rpx;
    display: flex;
    flex-direction: column;
    font-size: 0rpx;
    text-align: justify;
}

.bbs-box-reply>view {
    padding: 10rpx 0rpx;
}

.bbs-box-rtil {
    color: #576b95;
    font-size: 28rpx;
}

.bbs-box-rcon {
    font-size: 28rpx;
}

.bbs-box-rdel {
    color: #888888;
    font-size: 0rpx;
    display: flex;
    flex-wrap: wrap;
}

.bbs-box-rdel image {
    width: 32rpx;
    height: 32rpx;
    display: block;
    margin: 0rpx;
    padding: 0rpx;
    padding-right: 6rpx;
}

.bbs-box-rdel text {
    font-size: 28rpx;
    display: block;
    vertical-align: top;
}

.reply-editbg {
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 100%;
    background-color: black;
    z-index: 1001;
    opacity: 0.70;
    filter: alpha(opacity=70);
}

.reply-edit {
    width: 580rpx;
    position: fixed;
    top: 200rpx;
    left: 75rpx;
    right: 75rpx;
    margin: 0 auto;
    background-color: #fff;
    border-top: 2rpx solid #ddd;
    z-index: 1002;
    border-radius: 10rpx;
    overflow: hidden;
}

.reply-edit-til {
    text-align: center;
    font-size: 36rpx;
    line-height: 90rpx;
    margin-bottom: 20rpx;
    border-bottom: 1rpx solid #ddd;
}

.reply-edit-con {
    padding: 0rpx 40rpx 30rpx 40rpx;
}

.reply-edit-title {
    font-size: 32rpx;
    margin-bottom: 20rpx;
    color: #888888;
}

.reply-edit-input {
    padding: 10rpx;
    width: 480rpx;
    height: 240rpx;
    border: 1rpx solid #ddd;
}

.reply-edit-btn {
    display: flex;
    flex-wrap: nowrap;
    text-align: center;
}

.reply-edit-btn button {
    width: 50%;
    line-height: 100rpx;
    font-size: 36rpx;
}

.reply-edit-btnsub {
    color: #09bb07;
}

.bbs-list {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    padding: 20rpx 30rpx;
    border-bottom: 1rpx solid #ddd;
}

.bbs-list-headpic {
    width: 144rpx;
    height: 144rpx;
    border-radius: 8rpx;
}

.bbs-list-con {
    width: 520rpx;
}

.bbs-list-til {
    text-align: justify;
    min-height: 100rpx;
}

.bbs-list-info {
    font-size: 28rpx;
    color: #888888;
}

.bbs-list-info text {
    float: right;
}

.bbs-list-details {
    font-size: 28rpx;
    text-align: justify;
    width: 690rpx;
    margin-top: 20rpx;
    padding-top: 10rpx;
    color: #888888;
    border-top: 1rpx dashed #f0f0f0;
}

.bbs-list-irealname {
    font-weight: bold;
    color: #576b95;
}

.bbsuinfo {
    padding-top: 160rpx;
    padding-left: 40rpx;
    padding-bottom: 60rpx;
    border-bottom: 2rpx solid rgba(0,0,0,.3);
    font-size: 0rpx;
    display: flex;
    flex-direction: row;
    position: relative;
    overflow: hidden;
}

.bbsuinfo .bbsuinfo-bg {
    filter: blur(2px);
    position: absolute;
    left: 0rpx;
    top: 0rpx;
    z-index: -1;
    width: 70%;
    height: 100%;
}

.bbsuinfo .bbsuinfo-hp {
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
    border: 6rpx solid #fff;
}

.bbsuinfo text {
    line-height: 100rpx;
    font-size: 36rpx;
    margin-left: 36rpx;
    color: #fff;
    font-weight: bold;
}

.bbsnav {
    background-color: #fff;
}

.bbsnav .catelist-item {
    border-color: #ddd;
    color: #353535;
    display: flex;
}

.bbsnav .catelist-item image {
    width: 32rpx;
    height: 32rpx;
    margin-top: 29rpx;
    margin-right: 25rpx;
}

page {
    background-color: #f2f2f2;
    font-size: 16px;
    font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

.bgfff {
    background-color: #fff;
}